<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="collective.googledocsviewer">
<body>

 <div metal:fill-slot="content-core">

   <dl class="enableFormTabbing">
     <tal:letters tal:repeat="letter view/get_letters">
      <tal:words tal:define="words python:view.get_words(letter)">
        <dt tal:condition="words" tal:attributes="id string:fieldsetlegend-${letter}" tal:content="letter"></dt>
      </tal:words>
     </tal:letters>
     <tal:letters tal:repeat="letter view/get_letters">
      <tal:words tal:define="words python:view.get_words(letter)">
        <dd tal:condition="words" tal:attributes="id string:fieldset-${letter}">
          <ul>
            <li tal:repeat="word words">
             <span tal:content="word">word</span>:
             <ul>
              <li tal:repeat="page python:view.get_pages_for(word)">
                <a tal:attributes="href page/URL; title page/description" tal:content="page/title">title</a>
              </li>
             </ul>
            </li>
          </ul>
        </dd>
       </tal:words>
      </tal:letters>
   <dl>

 </div>
</body>
</html>